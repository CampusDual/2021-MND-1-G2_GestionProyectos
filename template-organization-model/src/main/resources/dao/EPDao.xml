<?xml version="1.0" encoding="UTF-8"?>
<JdbcEntitySetup
 xmlns="http://www.ontimize.com/schema/jdbc"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.ontimize.com/schema/jdbc http://www.ontimize.com/schema/jdbc/ontimize-jdbc-dao.xsd"
 catalog="" schema="${mainschema}" table="employee_project"
 datasource="mainDataSource" sqlhandler="dbSQLStatementHandler">
 <DeleteKeys>
  <Column>ep_id</Column>
 </DeleteKeys>
 <UpdateKeys>
  <Column>ep_id</Column>
 </UpdateKeys>
 <GeneratedKey>ep_id</GeneratedKey>
 
  <Queries>
  <Query id="info_projects">
   <AmbiguousColumns>
    <AmbiguousColumn name="employee" prefix="emp"
     databaseName="employee_id" />
    <AmbiguousColumn name="employee_project" prefix="employe"
     databaseName="employee_id" />
    <AmbiguousColumn name="project" prefix="pro"
     databaseName="project_id" />
    <AmbiguousColumn name="employee_project" prefix="emplo"
     databaseName="project_id" />
   </AmbiguousColumns>
   <Sentence>
   <![CDATA[
     SELECT
     #COLUMNS#
     FROM
      public.employee_project EP
     inner join public.employee e on
      EP.employe = e.emp
     inner join public.project p on
      EP.pro = p.emplo
     #WHERE#
   ]]>
   </Sentence>
  </Query>
 </Queries>
</JdbcEntitySetup>